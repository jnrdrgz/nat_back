Producto:
Endpoint: /productos/eliminar
PUT
parameters:
{
    "id": long -> id existente

}
-
return:
{
    "success": true,
    "data": {
        "producto": {
            "id": "968288cf-9d23-4b52-8bc7-314449a0ba67",
            "descripcion": "Eau de parfum femenino  rub",
            "codigo": 69615,
            "puntos": 0,
            "precio": 12780,
            "precioCosto": 0,
            "stock": 0,
            "foto": "a",
            "estaEliminado": true
        }
    }
}

Producto:
Endpoint: /productos/editar
PUT
parameters:
{
            "id": long -> id existente
            "descripcion": string,
            "codigo": long,
            "puntos": long,
            "precio": float,
            "precioCosto": float,
            "stock": float,
            "foto": string

}
-
return

{
    "success": true,
    "data": {
        "productoedit": {
            "id": "20a843b7-738d-4ac3-969e-098d068cbf72",
            "descripcion": "pepepepepepep",
            "codigo": 69615,
            "puntos": 0,
            "precio": 35195,
            "precioCosto": 0,
            "stock": 0,
            "foto": "a",
            "estaEliminado": false,
            "updatedAt": "2020-11-14T06:08:16.358Z"
        }
    }
}
