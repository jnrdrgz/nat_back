Endpoint: /pedidos/proveedor/agregar
POST:
parameters:
-
{
    "Pedido":{
        "total": float,
	    "DetallePedidos": [
		    {
			    "ProductoId": long,
			    "cantidad": float
		    },
            {
			    "Producto":{
                    "descripcion": string,
                    "codigo": long,
                    "puntos": long,
                    "precio": float,
                    "stock": float
                },
			    "cantidad": float
		    }
	    ]
    }
}

