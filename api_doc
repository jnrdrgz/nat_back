    "data": {
        "id": "37b7db41-b664-4feb-9c0b-55d8c231ab66",
        "ingresos": 0,
        "egresos": 0
    }
}










////////////PEDIDOS//////////////
Endpoint: /pedidos/cliente/agregar
POST:
parameters:
{
    "Pedido":{
        "total": float, // no tendrÃ­a que ir habria que sacarlo
	    "DetallePedidos": [
		    {
			    "productoId": ID de un producto existente,
			    "cantidad": float,
                "subtotal": float
		    },
            ... n canitadad de detalles

            !! IMPORTANTE !!
            tambien se puede cargar asi:
            SE PUEDEN COMBINAR LAS DOS FORMAS
            {
                {
                    "descripcion": string,
                    "codigo": long,
                    "puntos": long,
                    "precio": float,
                    "stock": float,
                }
			    "cantidad": float,
                "subtotal": float
		    },
	    ],

        ]
    },
	"Cliente": {
		"nombre": string,
		"numeroTelefono": string
	}
}
return
{
    "success": true,
    "data": {
        "id": "c853d9a8-0059-40b5-8274-b841cd4ff8f5",
        "montoSaldado": 0,
        "entregado": false,
        "pagado": false,
        "Pedido": {
            "id": "c39e6376-fafd-4e53-92dc-90cae896b4c1",
